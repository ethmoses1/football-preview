<h1><%= @club.name %></h1>

<ul>
  <h3>Players</h3>
  <% @club.players.each do |p| %>
  <li>
    <%= link_to p.plyname, player_path(p.id) %>
  </li>
  <%end %>
</ul>

<ul>
  <h3>Managers</h3>
  <% @club.employees.each do |e| %>
  <li>
    <%= e.name %>
  </li>
  <% end %>
</ul>

